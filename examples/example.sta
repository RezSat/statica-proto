# Example Statica workflow
 = load "examples/study.csv" with header

describe data

t1 = test ttest mean of data.score against 75

m1 = regress score ~ age + group on data

plot data.score histogram bins=10
plot data.age vs data.score scatter

conclude t1 alpha=0.05
conclude m1 alpha=0.05

